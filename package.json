{
  "name": "@saltcorn/visualize",
  "version": "0.7.0",
  "description": "Data visualization views plugin",
  "main": "visualize.js",
  "dependencies": {
    "@saltcorn/markup": "^0.4.0",
    "@saltcorn/data": "^0.4.0"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "repository": "github:saltcorn/visualize",
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-case-declarations": "off",
      "no-empty": "warn",
      "no-fallthrough": "warn"
    }
  }
}
